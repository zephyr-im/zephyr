LIBS= $(ZEPHYR_LIB) $(COMERR_LIB) $(KRB_LIB)
LINCLUDES=-I.
LINTLIBS=$(ZEPHYR_LINTLIB) $(COMERR_LINTLIB) $(KRB_LINTLIB)
